{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "camelcase":"off",
        "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
        "quotes": [2, "double", { "avoidEscape": true }],
        "linebreak-style": ["error", "windows"],
        "no-console": "off"
    }
}
